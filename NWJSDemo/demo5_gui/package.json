{
    "name": "gui-demo",
    "main": "index.html",
    "description": "demo app of node-webkit",
    "version": "1.0.0",
    "keywords": [ "demo", "node-webkit" ],
    "nodejs": true,
    "node-main": "index.js",
    "window":{
        "title":"默认设置",
        "icon":"logo.png",
        "toolbar":true,
        "frame": true,
        "position": "center",
        "width":400,
        "height":300,
        "resizable":true,
        "show_in_taskbar":true,
        "always-on-top":true,
        "fullscreen":false,
        "kiosk":false
    },
    "webkit":{
        "plugin":true
    },
    "user-agent": "测试 %ver %nwver %webkit_ver windows8.1" /* 替换占位符内容即可 */
}